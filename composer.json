{
    "name": "mohammadalavi/laravel-shoppingcart",
    "description": "A Shopping Cart Implementation for Laravel",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Shop",
        "Cart",
        "E-Commerce",
        "Shopping Cart"
    ],
    "authors": [
        {
            "name": "Mohammad Alavi",
            "email": "mohammad.alavi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.28.1",
        "moneyphp/money": "^3.3.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "MohammadAlavi\\ShoppingCart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohammadAlavi\\ShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "ShoppingCart": "MohammadAlavi\\ShoppingCart\\Facades\\ShoppingCart"
            }
        }
    }
}